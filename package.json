{
  "name": "@oneverse/identify",
  "description": "OneVerse Identify SDK",
  "version": "1.4.0",
  "license": "MIT",
  "author": "Sean",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "files": [
    "es",
    "lib",
    "package.json",
    "README.md"
  ],
  "exports": {
    "./services/*": {
      "require": "./lib/services/*.js",
      "import": "./es/services/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:mjs": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "yarn clean && yarn format && yarn build:cjs && yarn build:mjs",
    "clean": "rm -rf es && rm -rf lib",
    "format": "prettier --write \"src/**/*.[jt]s\"",
    "release": "semantic-release --debug --ci=false",
    "prepare": "yarn build"
  },
  "optionalDependencies": {
    "@3id/did-provider": "^0.4.1",
    "@3id/model": "^0.4.1",
    "@ceramicnetwork/http-client": "^2.2.1",
    "@glazed/did-datastore": "^0.3.2",
    "@oneverse/utils": "^1.1.0",
    "did-jwt-vc": "^2.1.13",
    "dids": "^3.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.2",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:oneverse-zone/identity.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
