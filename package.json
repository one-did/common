{
  "name": "@oneverse/identify",
  "description": "OneVerse Identify SDK",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Sean",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "files": [
    "es",
    "lib",
    "package.json",
    "README.md"
  ],
  "exports": {
    "./services/": {
      "require": "./lib/services/*.js",
      "import": "./es/services/*.js"
    },
    "./utils/*": {
      "require": "./lib/utils/*.js",
      "import": "./es/utils/*.js"
    }
  },
  "scripts": {
    "build:mjs": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "yarn build:cjs && yarn build:mjs",
    "format": "prettier --write \"src/**/*.[jt]s\"",
    "release": "semantic-release --debug --ci=false",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@3id/did-provider": "^0.4.1",
    "@3id/model": "^0.4.1",
    "@ceramicnetwork/http-client": "^2.0.4",
    "@glazed/did-datastore": "^0.3.1",
    "did-jwt-vc": "^2.1.12",
    "dids": "^3.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.1",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:oneverse-zone/identity.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
